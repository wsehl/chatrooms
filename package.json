{
  "version": "0.0.1",
  "scripts": {
    "dev:client": "vite",
    "build:client": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "dev:server": "nodemon",
    "build:server": "rimraf dist/server && tsc -p tsconfig.server.json",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build": "concurrently \"npm run build:server\" \"npm run build:client\"",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write .",
    "heroku-postbuild": "npm run build:server"
  },
  "nodemonConfig": {
    "watch": [
      "./server/"
    ],
    "ext": "ts",
    "ignore": [
      "**/*.test.ts"
    ],
    "exec": "ts-node --project ./tsconfig.server.json --files ./server/index.ts"
  },
  "dependencies": {
    "express": "^4.17.2",
    "socket.io": "^4.4.1",
    "vue": "^3.2.27",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/node": "^16.11.21",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "~4.5.4",
    "vite": "^2.7.13",
    "vue-tsc": "^0.29.8"
  }
}